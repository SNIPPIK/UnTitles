########################################################################################################
### Status System                                                                                    ###
### Управление настройками статуса                                                                   ###
########################################################################################################
# Поддерживаются | online, idle, dnd, invisible
client.status="online"

# Поддерживаются | WEB, Discord iOS, Discord Android
# Внимание этот метод нарушает политику Discord API
client.browser=""

# Время обновления статуса бота в секундах
client.presence.interval=35

# Используется JSON
# Поддерживаются type | Playing, Streaming, Listening, Watching, Custom, Competing
# Поддерживаются типы {guilds}, {version}, {queues}, {players}, {shard}, {users}
# Необходимо включить `Server Members Intent`
client.presence.array={"name": "version {version}", "type": "Watching"}, {"name": "shard {shard} on {guilds} guilds", "type": "Watching"}, {"name": "on {players} guilds", "type": "Listening"}

# Время обновления кастомных статусов
client.presence.array.update=60

########################################################################################################
### Cooldown System                                                                                  ###
### Управление ограничениями при обработке запросов                                                  ###
########################################################################################################
cooldown=on

# Ограничение по времени при обработке команд или кнопок в секундах
cooldown.time=5


########################################################################################################
### Language System                                                                                  ###
### Управление языковыми настройками                                                                 ###
### Надо учитывать какие языки доступны. Есть [ru, en-US] | Для авто выбора - null, не "null"        ###
########################################################################################################
language.command=null                  # Названия команд будут на указанном языке

########################################################################################################
### Whitelist Sub System                                                                             ###
### Если включен то бот будет реагировать только на указанных пользователей                          ###
### Доступное значение: string,string                                                                ###
########################################################################################################
whitelist=off
whitelist.list=""

########################################################################################################
### Blacklist Sub System                                                                             ###
### Если включен то бот будет полностью игнорировать этих пользователей                              ###
### Доступное значение: string,string                                                                ###
########################################################################################################
blacklist=off
blacklist.list=""

########################################################################################################
### Webhook Sub System                                                                               ###
### Если включен то бот при получении ошибки будет выводить ошибку в канал                           ###
### Доступное значение: string                                                                       ###
########################################################################################################
webhook.id=""
webhook.token=""

########################################################################################################
### Owner Sub System                                                                                 ###
### Команды доступные для разработчиков будут доступны только этом пользователям                     ###
### Доступное значение: string                                                                       ###
########################################################################################################
owner.server=""                 # Сервер где будут доступны особые команды
owner.list=""                   # Список разработчиков, указывать через -> ,

########################################################################################################
### Дополнительные настройки плеера, могут сильно повлиять на качество звука                         ###
