{
  "name": "untitles",
  "version": "0.4.1",
  "homepage": "https://github.com/SNIPPIK/UnTitles",
  "bugs": "https://github.com/SNIPPIK/UnTitles/issues",
  "author": {
    "name": "SNIPPIK",
    "url": "https://github.com/SNIPPIK"
  },
  "license": "BSD-3-Clause + custom restriction",
  "preferGlobal": true,
  "engineStrict": true,
  "types": "src/index.ts",
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "discord-api-types": "^0.38.36",
    "discord.js": "14.25.1",
    "ws": "^8.18.3"
  },
  "optionalDependencies": {
    "@snazzah/davey": "^0.1.8"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "start": "cd build && node -r tsconfig-paths/register src --ShardManager --optimize_for_size --expose-gc --experimental-require-module --no-compilation-cache",
    "start-bun": "bun src",
    "build": "tsc",
    "dev:start": "npm run build && cd build && node -r tsconfig-paths/register src --optimize_for_size --expose-gc",
    "dashboard": "npm run build && cd build && node -r tsconfig-paths/register web --optimize_for_size --expose-gc --experimental-require-module --no-compilation-cache"
  }
}
